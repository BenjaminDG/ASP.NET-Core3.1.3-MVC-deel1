
<h1>winkelmanje</h1>




@model List<Brochure>

<table class ="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Bestemming</th>
        </tr>
    </thead>
    <tbody>
        
        @foreach( var brochure in Model)
        {
        <tr>
            <td>@Html.DisplayFor(m => brochure.ID)</td>
            
            <td>@Html.DisplayFor(m=>brochure.Bestemming)</td>
            
        </tr>
        }
    </tbody>



    </table>